{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "nodemon dist-server/server.js",
    "start:prod": "cross-env NODE_ENV=production node -r dotenv/config ./dist-server/server.js",
    "dev:server": "cross-env NODE_ENV=development nodemon server.ts",
    "build:server": "tsc --project tsconfig.server.json"
  },
  "dependencies": {
    "@google-ai/generativelanguage": "^1.1.0",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.14.9",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "googleapis": "^140.0.0",
    "html-pdf": "^3.0.1",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.14",
    "pdfkit": "^0.15.0",
    "pg": "^8.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/html-to-text": "^9.0.4",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/nodemailer": "^6.4.15",
    "@types/pg": "^8.11.6",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "nodemon": "^3.1.4",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}